{
  "name": "uzzalhcse/amadeus-ws-client",
  "type": "library",
  "description": "SOAP Web Service client library for interacting with the Amadeus GDS through its SOAP interface",
  "keywords": ["amadeus", "gds", "service", "webservice", "soap", "wsdl", "client", "xml", "travel", "airline", "hotel", "booking", "car", "rental", "offer", "masterpricer"],
  "minimum-stability": "dev",
  "prefer-stable": false,
  "homepage": "https://github.com/amabnl/amadeus-ws-client",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Uzzal Hosen",
      "email": "uzzalh.cse@gmail.com",
      "homepage": "",
      "role": "Principal Software Enginner"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-xsl": "*",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "Amadeus\\": "src/Amadeus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Amadeus\\": "tests/Amadeus"
    }
  },
  "config": {
    "platform": {
      "php": "8.2|^8.1"
    }
  }
}
